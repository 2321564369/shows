function showDialog(n){$(function(){$(n).dialog({autoOpen:!0,modal:!0,width:736,open:function(o,i){$(".ui-widget-overlay").bind("click",function(){$(n).dialog("close")}),$("body").addClass("overflowhidecs")},close:function(o,i){$("body").removeClass("overflowhidecs")}})})}function showGroupButton(o){showDialog("#comment-full-"+o)}
